<?xml version="1.0" encoding="utf-8"?>
<manifest>
  <schemas>
    <schema id="org.opent2t.sample.hub.superpopular" main="true" type="OCF" />
    <schema id="oic.core" type="OCF" />
    <schema id="oic.baseResource" type="OCF" />
  </schemas>
  <!-- associated onboarding module -->
  <onboarding id="org.opent2t.onboarding.wifi.wemo">
    <onboardflow name="getUserInput">
      <flow type="input">
        <arg name="ssid" />
        <description language="en">What WiFi network do you want to use?</description>
      </flow>
      <flow type="password">
        <arg name="password"  />
        <description language="en">Type in your WiFi password</description>
      </flow>
      <flow type="input">
        <arg name="controlId" />
        <description language="en">What is the device ID?</description>
      </flow>
    </onboardflow>
  </onboarding>
</manifest>